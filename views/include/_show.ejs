<% for(var i = users.length - 1; i >= 0; i--) { %>
<tr>
    <td><%= users.length - i %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].age %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].diagnose %></td>
    <td><%= users[i].timestamp%></td>
    <td>
            <a href="/tra-cuu?id=<%=i%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fa-solid fa-magnifying-glass"></i></span>
            </a>
            <a class="delete" data-id=<%= users[i]._id%>>
                <span class="text-gradient"><i class="fa-solid fa-check"></i></span>
            </a>
            <!-- <button class="chat_flowup">

            </button> -->
    </td>
</tr>
<% } %>